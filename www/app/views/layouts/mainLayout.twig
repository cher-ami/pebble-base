<!doctype html>
<html>
<head>
	<meta http-equiv="x-ua-compatible" content="IE=Edge"/>
	<meta http-equiv="imagetoolbar" content="no" />
	<meta charset="UTF-8">

	{% include 'partials/components/headerBanner.twig' %}

	<!-- META -->
	<title>{{ vars.title }}</title>
	<meta name="description" content="{{ vars.description }}" />

	<!-- FB -->
	<meta property="og:type"          	content="website" />
	<meta property="og:url" 			content="{{ config.server.host }}" />
	<meta property="og:title"         	content="{{ vars.title }}" />
	<meta property="og:description"   	content="{{ vars.description }}" />
	<meta property="og:image"         	content="{{ config.server.host ~ asset('share.jpg', 'img') }}" />
	<meta property="og:image:height"    content="628" />
	<meta property="og:image:height"    content="1200" />

	<!-- TWITTER -->
	<meta name="twitter:card" content="summary" />
	<meta name="twitter:title" content="{{ vars.title }}" />
	<meta name="twitter:description" content="{{ vars.description }}" />
	<meta name="twitter:image" content="{{ config.server.host ~ asset('share.jpg', 'img') }}" />

	<!-- ICONS -->
	<link rel="apple-touch-icon" sizes="57x57" href="{{ asset('apple-touch-icon-57x57.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="60x60" href="{{ asset('apple-touch-icon-60x60.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="72x72" href="{{ asset('apple-touch-icon-72x72.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="76x76" href="{{ asset('apple-touch-icon-76x76.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="114x114" href="{{ asset('apple-touch-icon-114x114.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="120x120" href="{{ asset('apple-touch-icon-120x120.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="144x144" href="{{ asset('apple-touch-icon-144x144.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="152x152" href="{{ asset('apple-touch-icon-152x152.png', 'img') }}">
	<link rel="apple-touch-icon" sizes="180x180" href="{{ asset('apple-touch-icon-180x180.png', 'img') }}">

	<!-- STYLES -->
	<link rel="stylesheet" href="{{ asset('common.css', 'css') }}" />
	<link rel="stylesheet" href="{{ asset('module-name.css', 'css') }}" />

	<!-- VIEWPORT -->
	<meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width" />
</head>
<body>
	<!-- PRE-LOADER -->
	<div class="Preloader">
		<div class="Preloader_loader"></div>
	</div>

	<!-- SCRIPTS -->
	<script src="{{ asset('static-libs.js', 'js') }}"></script>
	<script src="{{ asset('common.js', 'js') }}"></script>
	<script src="{{ asset('module-name.js', 'js') }}"></script>

	<!-- PAGE -->
	{% block content %}{% endblock %}

	<!-- TRACKING -->
	<script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
				(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
		ga('create', '{{ config.analytics.trackingCode }}', 'auto');
		ga('send', 'pageview');
	</script>
</body>
</html>